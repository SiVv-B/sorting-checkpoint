ALGORITHM tri_a_bulles
VAR
    element : ARRAY_OF INTEGER;
    i,j,temp: INTEGER;

BEGIN
    FOR i FROM -1 TO 1 step 1 DO
    FOR j FROM 1 TO i DO
    IF ( elemnt[j] > elemnt[j+1] ) THEN
        temp := elemnt[j];
        elemnt[j]:=elemnt[j+1];
        elemnt[j+1]:=temp;
    END_IF
    END_FOR
    END_FOR
END